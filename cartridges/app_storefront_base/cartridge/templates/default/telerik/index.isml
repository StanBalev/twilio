	<div class="modal fade" id="exampleModalToggle" aria-hidden="true" aria-labelledby="exampleModalToggleLabel"
		tabindex="-1">
		<div class="modal-dialog modal-dialog-centered">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="addressModalLabel">Enter your details below. All fields are required
					</h5>
					<p class="text-center"></p>
					<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
				</div>
				<div class="modal-body">
					<div class="modal-body">
						<form>
							<div class="row">
								<div class="col">
									<label for="address1" class="col-form-label">Address:</label>
									<input type="text" required class="form-control" required id="address1">
								</div>
							</div>
							<div class="row">
								<div class="col">
									<label for="city" class="col-form-label">City:</label>
									<input type="text" class="form-control" required id="city">
								</div>
								<div class="col">
									<label for="countryCode" class="col-form-label">Country Code:</label>
									<input type="text" class="form-control" required id="countryCode">
								</div>
							</div>
							<div class="row">
								<div class="col">
									<label for="firstName" class="col-form-label">First Name:</label>
									<input type="text" class="form-control" required id="firstName">
								</div>
								<div class="col">
									<label for="lastName" class="col-form-label">Last Name:</label>
									<input type="text" class="form-control" required id="lastName">
								</div>
							</div>
							<div class="row">
								<div class="col">
									<label for="postalCode" class="col-form-label">Postal Code:</label>
									<input type="text" class="form-control" required id="postalCode">
								</div>
								<div class="col">
									<label for="phoneNumber" class="col-form-label">Phone Number:</label>
									<input type="text" class="form-control" required id="phoneNumber">
								</div>
							</div>
						</form>
					</div>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
					<button class="btn btn-primary" data-bs-target="#exampleModalToggle2" onclick="setBillingAddress()"
						data-bs-toggle="modal" data-bs-dismiss="modal">Next</button>
				</div>
			</div>
		</div>
	</div>
	<div class="modal fade" id="exampleModalToggle2" aria-hidden="true" aria-labelledby="exampleModalToggleLabel2"
		tabindex="-1">
		<div class="modal-dialog modal-dialog-centered">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="exampleModalToggleLabel2">Payment</h5>
					<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
				</div>
				<div class="modal-body">
					<form>
						<div class="row">
							<div class="col">
								<label for="cardNumber" class="col-form-label">Card#:</label>
								<input type="text" required class="form-control" required id="cardNumber">
								<label for="nameOnCard" class="col-form-label">Name:</label>
								<input type="text" class="form-control" required id="nameOnCard">
							</div>
						</div>
						<div class="row">
							<div class="col">
								<label for="expirationmm" class="col-form-label">Expiration MM:</label>
								<input type="text" class="form-control" required maxlength="2" id="expirationmm">
							</div>
							<div class="col">
								<label for="expirationyy" class="col-form-label">Expiration YYYY:</label>
								<input type="text" class="form-control" required maxlength="4" id="expirationyy">
							</div>
							<div class="col">
								<label for="secCode" class="col-form-label">Security Code:</label>
								<input type="text" maxlength="3" required class="form-control" id="secCode">
							</div>
						</div>

					</form>
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
					<button class="btn btn-primary" data-bs-target="#exampleModalToggle" data-bs-toggle="modal"
						data-bs-dismiss="modal">Back</button>
					<button type="button" onclick="setPaymentMethod()" class="btn btn-primary">Place order</button>
				</div>
			</div>
		</div>
	</div>
	<a class="btn btn-primary d-none" id="openFirstModal" data-bs-toggle="modal" href="#exampleModalToggle"
